   83  cp d:/wx/dlam.jpg MYX/
   84  ls MYX
   85  rm MYX/dlam.jpg dybl.jpg -r
   86  cp d:/wx/dlam.jpg MYX/
   87  rm MYX dlam.jpg dybl.jpg -r
   88  ls
   89  mkdir MYX/
   90  ls
   91  history 
   92  ls
